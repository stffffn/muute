{
  "name": "muute",
  "version": "0.1.0",
  "description": "Extension for Chrome based browsers for global keyboard shortcuts in Google Meet",
  "scripts": {
    "start": "web-ext run --target=chromium --source-dir=./extension --start-url=https://meet.google.com/",
    "build": "web-ext build --source-dir=extension --artifacts-dir=build"
  },
